<?xml version="1.0" encoding="utf-8"?>

<word_cnt_importer>
<!-- bigpipe config
    <BigPipe>
      <bigpipe_name>
         <Authentication></Authentication>
         <Token>bigpipe_token</Token>
         <Pipelet>0-2</Pipelet>
         <PipeletNum>3</PipeletNum>
         <SubPoint>1</SubPoint>
      </bigpipe_name>
    </BigPipe>
-->
    <LocalFile>
      <FileName>./data/local_file</FileName>
      <SubPoint>0</SubPoint>
      <!-- ReadMethod -->
      <!-- 1. ReadToEnd -->
      <!--      : read local file only once to end -->
      <!-- 2. ReadLoop -->
      <!--      :loop read local file -->
      <ReadMethod>ReadToEnd</ReadMethod>
    </LocalFile>
</word_cnt_importer>
