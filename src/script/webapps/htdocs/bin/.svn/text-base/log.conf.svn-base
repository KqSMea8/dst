log4cplus.rootLogger=TRACE,LOG2FILE  
#log to file 
log4cplus.appender.LOG2FILE=log4cplus::RollingFileAppender  
log4cplus.appender.LOG2FILE.File=doodle.log  
log4cplus.appender.LOG2FILE.MaxFileSize=1MB 
log4cplus.appender.LOG2FILE.layout=log4cplus::PatternLayout 
log4cplus.appender.LOG2FILE.layout.ConversionPattern=[%D{%Y-%m-%d %H:%M:%S}][%p][%c]%x:%m%n
#log to stdout console
log4cplus.logger.metrics_dumper=DEBUG,LOG2FILE
log4cplus.logger.pn_debugger=DEBUG,LOG2FILE
log4cplus.logger.ZK_META_DUMPER=DEBUG,LOG2FILE
