<?xml version="1.0" encoding="utf-8"?>

<APP>
	<AppName>word_count_test</AppName>
	<Description>new added sub tree for experiment</Description>

	<Topology>
		<ProcessorNum>4</ProcessorNum>
		<Importer>
			<Importer1>
				<ProcessorName>echo_importer_add1</ProcessorName>
				<RunCmd>./bin/export</RunCmd>
				<Resource>
					<CPU>0.5</CPU>
					<Memory>16000</Memory>
				</Resource>
				<Parallelism>1</Parallelism>
				<BackupPENum>1</BackupPENum>
        <StatusMark>0</StatusMark>
				<TuplesBatchCount>10</TuplesBatchCount>				
			</Importer1>
			<Importer2>
				<ProcessorName>echo_importer_add2</ProcessorName>
				<RunCmd>./bin/export</RunCmd>
				<Resource>
					<CPU>0.5</CPU>
					<Memory>16000</Memory>
				</Resource>
				<Parallelism>1</Parallelism>
				<BackupPENum>1</BackupPENum>
        <StatusMark>0</StatusMark>
				<TuplesBatchCount>10</TuplesBatchCount>				
			</Importer2>
			<Importer3>
				<ProcessorName>echo_importer_add3</ProcessorName>
				<RunCmd>./bin/export</RunCmd>
				<Resource>
					<CPU>0.5</CPU>
					<Memory>16000</Memory>
				</Resource>
				<Parallelism>1</Parallelism>
				<BackupPENum>1</BackupPENum>
        <StatusMark>0</StatusMark>
				<TuplesBatchCount>10</TuplesBatchCount>				
			</Importer3>
    </Importer>
    <Exporter>
      <Exporter1>
        <ProcessorName>echo_exporter</ProcessorName>
        <RunCmd>./bin/exporter</RunCmd>
        <Resource>
          <CPU>0.5</CPU>
          <Memory>16000</Memory>
        </Resource>
        <Parallelism>1</Parallelism>
        <BackupPENum>1</BackupPENum>
        <StatusMark>0</StatusMark>
        <TuplesBatchCount>10</TuplesBatchCount>
        <Subscribe>
          <source>
            <processor>echo_importer_add1</processor>
          </source>
          <source>
            <processor>echo_importer_add2</processor>
          </source>
          <source>
            <processor>echo_importer_add3</processor>
          </source>
        </Subscribe>
      </Exporter1>
    </Exporter>
	</Topology>
</APP>
